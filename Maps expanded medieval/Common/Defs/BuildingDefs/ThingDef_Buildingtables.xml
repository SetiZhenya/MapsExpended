<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="CustomToolBase">
        <defName>Seti_Loot_tablemeat</defName>
        <label>table (meat)</label>
        <description>A table with meat, can be looted.</description>
        <thingClass>QuestEditor_Library.LootBox</thingClass>
        <category>Building</category>
        <rotatable>false</rotatable>
        <graphicData>
			<texPath>Tents/MarketMeat</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(5,3)</drawSize>
        </graphicData>
		<size>(3,1)</size>
        <costStuffCount>35</costStuffCount>
        <statBases>
            <Mass>20</Mass>
        </statBases>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <expandHomeArea>true</expandHomeArea>
            <claimable>true</claimable>
        </building>
        <modExtensions>
            <li Class="QuestEditor_Library.ModExtension_CustomThing">
                <openedGraphicdata>
					<texPath>Furniture/RusticTable1x3_east</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(5,3)</drawSize>
                </openedGraphicdata>
            </li>
        </modExtensions>
    </ThingDef>
	
