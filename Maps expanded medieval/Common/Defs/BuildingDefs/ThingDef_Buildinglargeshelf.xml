<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="CustomToolBase">
        <defName>Seti_Loot_largeshelf_bread</defName>
        <label>large bread shelf</label>
        <description>A large bread shelf, can be looted.</description>
        <thingClass>QuestEditor_Library.LootBox</thingClass>
        <category>Building</category>
        <rotatable>false</rotatable>
        <graphicData>
			<texPath>Storage/StackBread</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(4,4)</drawSize>
        </graphicData>
        <costStuffCount>35</costStuffCount>
        <statBases>
            <Mass>20</Mass>
        </statBases>
		<size>(2,2)</size>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <expandHomeArea>true</expandHomeArea>
            <claimable>true</claimable>
        </building>
        <modExtensions>
            <li Class="QuestEditor_Library.ModExtension_CustomThing">
                <openedGraphicdata>
					<texPath>Storage/StackEmpty</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(4,4)</drawSize>
                </openedGraphicdata>
            </li>
        </modExtensions>
    </ThingDef>
	
