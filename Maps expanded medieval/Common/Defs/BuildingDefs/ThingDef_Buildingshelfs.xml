<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="CustomToolBase">
        <defName>Seti_Loot_shelfmedic</defName>
        <label>medical shelf</label>
        <description>A shelf with medical supplies, can be looted.</description>
        <thingClass>QuestEditor_Library.LootBox</thingClass>
        <category>Building</category>
        <rotatable>false</rotatable>
        <graphicData>
			<texPath>Storage/ShelfHerb</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.75,2.75)</drawSize>
			<shaderType>CutoutComplex</shaderType>
        </graphicData>
		<size>(2,1)</size>
        <statBases>
            <Mass>20</Mass>
        </statBases>
		<costStuffCount>35</costStuffCount>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <expandHomeArea>true</expandHomeArea>
            <claimable>true</claimable>
        </building>
        <modExtensions>
            <li Class="QuestEditor_Library.ModExtension_CustomThing">
                <openedGraphicdata>
					<texPath>Storage/ShelfEmpty</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(2.75,2.75)</drawSize>
                </openedGraphicdata>
            </li>
        </modExtensions>
    </ThingDef>
	
